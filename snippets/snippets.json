{
  "1 row 2 cols": {
    "prefix": "rg-1r2c",
    "body": [
      "<!-- FULL REFERENCE: https://inarocket.com/resources/cssgrid-layouts -->",
      "<!-- CSS Code: Start -->",
      "*,",
      "*::before,",
      "*::after { box-sizing: border-box; }",
      "",
      ":root {",
      "  --color-primary: #2c5282;",
      "  --color-secondary: #c2d4ea;",
      "}",
      "",
      ".grid-container {",
      "  display: grid;",
      "  grid: auto / repeat(2, 1fr);",
      "  gap: .5rem;",
      "  padding: .5rem;",
      "  background: var(--color-primary);",
      "}",
      "",
      ".grid-container > * { background: var(--color-secondary); }",
      "<!-- CSS Code: End -->",
      "",
      "<!-- HTML Code: Start -->",
      "<div class=\"grid-container\">",
      "  <div>Item 1</div>",
      "  <div>Item 2</div>",
      "</div>",
      "<!-- HTML Code: End -->"
    ],
    "description": "Grid: 1 row 2 cols"
  },
