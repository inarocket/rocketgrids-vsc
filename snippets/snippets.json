  "2 rows 2 cols / rowspan left": {
    "prefix": "rg-2r2c-rsl",
    "body": [
      "<!-- FULL REFERENCE: https://inarocket.com/resources/cssgrid-layouts -->",
      "<!-- CSS Code: Start -->",
      "*,",
      "*::before,",
      "*::after { box-sizing: border-box; }",
      "",
      ":root {",
      "  --color-primary: #2c5282;",
      "  --color-secondary: #c2d4ea;",
      "}",
      "",
      ".i1 { grid-area: a1; }",
      ".i2 { grid-area: a2; }",
      ".i3 { grid-area: a3; }",
      "",
      ".grid-container {",
      "  display: grid;",
      "  grid: repeat(2, 1fr) / repeat(2, 1fr);",
      "  gap: .5rem;",
      "  padding: .5rem;",
      "  background: var(--color-primary);",
      "  /* With areas */",
      "  grid-template-areas:  \"a1 a2\"",
      "                        \"a1 a3\";",
      "}",
      "",
      "/* With lines */",
      "/* Uncomment the following code and comment grid-template-areas to get the same layout using lines instead of areas */",
      "/* .i1 { ",
      "  grid-row: 1 / -1;",
      "  grid-column: 1;",
      "}",
      ".i2 { ",
      "  grid-row: 1;",
      "  grid-column: 2;",
      "}",
      ".i3 { ",
      "  grid-row: 2;",
      "  grid-column: 2;",
      "} */",
      "",
      ".grid-container > * { background: var(--color-secondary); }",
      "<!-- CSS Code: End -->",
      "",
      "<!-- HTML Code: Start -->",
      "<div class=\"grid-container\">",
      "  <div class=\"i1\">Item 1</div>",
      "  <div class=\"i2\">Item 2</div>",
      "  <div class=\"i3\">Item 3</div>",
      "</div>",
      "<!-- HTML Code: End -->",
      ""
    ],
    "description": "Grid: 2 rows 2 cols / rowspan left"
  },
